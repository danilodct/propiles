			<interceptor-ref name="defaultPilatesStack-SemLogin"></interceptor-ref>
			<result name="success" type="chain">home</result>
			<result name="error">pages/login.jsp</result>
		</action>
		<action name="logout" class="br.com.pilates.actions.LoginLogoutAction" method="actionLogout">
